<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Ingresar con Google", omniauth_authorize_path(resource_name, provider), class:'text-blue-500 lg:text-xl'  %><br />
  <% end %>
<% end %>

<%- if controller_name != 'sessions' %>
  <%= link_to "Log in", new_session_path(resource_name), class:'text-blue-500 lg:text-xl' %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Crear cuenta nueva", new_registration_path(resource_name), class:'text-blue-500 lg:text-xl' %><br />
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Olvide mi contraseña", new_password_path(resource_name), class:'text-blue-500 lg:text-xl' %><br />
<% end %>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "No me llego ningún correo de confirmación", new_confirmation_path(resource_name), class:'text-blue-500 lg:text-xl' %><br />
<% end %>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name), class:'text-blue-500 lg:text-xl'  %><br />
<% end %>

